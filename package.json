{
  "name": "tamariver",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "v20.18.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rm -r ./.next ./node_modules ./.vercel ; npm i",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "format:check": "npm run format; git diff HEAD --exit-code",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:preview": "npm run pages:build && wrangler pages dev .vercel/output/static --compatibility-date=2024-01-29 --compatibility-flag=nodejs_compat",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "gen": "plop"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.3",
    "lru-cache": "^11.0.1",
    "nanoid": "^5.0.7",
    "next": "14.2.15",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@cloudflare/workers-types": "^4.20241011.0",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-onboarding": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/nextjs": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-next-on-pages": "^1.8.6",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "plop": "^4.0.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "storybook": "^8.3.5",
    "tailwindcss": "^3.4.13",
    "typescript": "^5",
    "vercel": "^37.8.0",
    "wrangler": "^3.80.4"
  }
}
